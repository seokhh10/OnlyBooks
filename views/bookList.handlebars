{{!-- GET the list of books in the table "books" and render them--}}

<!-- Loop over each object in the book array -->
{{#each books as |book| }}
<div class="row mb-4 book">
  <div class="col-md-5">
    <h2>
      <a href="/api/reviews/book/{{book.id}}">{{book.title}}</a>
    </h2>
  </div>
</div>
{{/each}}
